if [ -z "$XDG_CONFIG_DIRS" ]; then
   export XDG_CONFIG_DIRS=/usr/share/kde-mouse-doubleclick/
else
   export XDG_CONFIG_DIRS=/usr/share/kde-mouse-doubleclick/:$XDG_CONFIG_DIRS
fi
